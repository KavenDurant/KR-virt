{
  "name": "kr-virt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:pro": "vite --mode production",
    "build": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "build:analyze": "npm run build:prod && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,less}\"",
    "format-all": "node scripts/format-all.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "docker:build": "docker build -t kr-virt .",
    "docker:run": "docker run -p 3000:80 kr-virt",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose --profile production up --build -d"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "antd": "^5.25.2",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "less-loader": "^12.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-idle-timer": "^5.7.2",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^3.0.2",
    "react-router-dom": "^7.6.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.0.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.1",
    "@vitest/ui": "^3.2.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "less": "^4.3.0",
    "terser": "^5.40.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  }
}
